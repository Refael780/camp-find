<% include partials/header.ejs%>
<div class="container">
    <div class="jumbotron">
        <div class="container">
            <h3>Login</h3>
        </div>
    </div>
</div>

</div>
<div class="container">
    <div class="container">

        <form action="/login" method="post" class="form-horizontal">
            <div class="form-group">
                <label for="inputName" class="col-sm-4 control-label">Username</label>
                <div class="col-sm-8">
                    <input type="text" placeholder="Username" name="username" class="form-control">
                </div>
            </div>
            <div class="form-group">
                <label for="inputImage" class="col-sm-4 control-label">Password</label>
                <div class="col-sm-8">
                    <input type="password" placeholder="image" name="password" class="form-control" id="inputImage">
                </div>
            </div>
            <div class="form-group">
                <button class="btn btn-primary btn-lg ">Submit</button>
            </div>
        </form>
    </div>


</div>
<% include partials/footer%>